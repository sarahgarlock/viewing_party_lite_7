<h1>Viewing Party</h1>

<% if current_user %>
  <%= link_to "Log Out" %>
<% else %>
  <%= link_to 'Log In', login_path %>
  <%= button_to 'Create New User', '/register', method: :get %>
<% end %>


<h2>Existing Users</h2>
<section  id="existing-users"> 
<% @users.each do |user| %>
  <ul> 
    <li id="user-<%= user.id %>"> <%= link_to user.name, "/users/#{user.id}" %> </li>
  </ul>  
<% end %>
</section>
